{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "semicolon": [true, "never"],
        "quotemark": [true, "single", "avoid-escape"],
        "object-literal-sort-keys": false,
        "whitespace": [true],
        "max-line-length": [
            true,
            {
                "limit": 200,
                "ignore-pattern": "// {(.*?)}",
                "check-strings": true,
                "check-regex": true
            }
        ]
    },
    "rulesDirectory": []
}
